<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="palisson.bdeb.qc.ca.bdebgarde">

    <application
        android:name=".CampDeJour"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Accueil">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginParent"
            android:parentActivityName=".Accueil"/>
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Accueil" />

        <activity android:name=".ListeParent" />
        <activity android:name=".ParentVueEnfantActivity"></activity>
    </application>

</manifest>